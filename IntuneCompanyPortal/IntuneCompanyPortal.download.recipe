<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
    <key>Description</key>
    <string>Downloads the Intune Company Portal dmg file.</string>
    <key>Identifier</key>
    <string>com.github.yohan460-recipes.download.IntuneCompanyPortal</string>
    <key>Input</key>
    <dict>
        <key>NAME</key>
        <string>InTune Company Portal</string>       
        <key>DOWNLOAD_URL</key>
        <string>https://download.microsoft.com/download/3/D/C/3DC456DB-0679-435D-A2A3-05CE193E34C5/CompanyPortal.dmg</string>
    </dict>
    <key>MinimumVersion</key>
    <string>1.0.0</string>
    <key>Process</key>
    <array>
        <dict>
            <key>Processor</key>
            <string>URLDownloader</string>
            <key>Arguments</key>
            <dict>
                <key>url</key>
                <string>%DOWNLOAD_URL%</string>
            </dict>
        </dict>
        <dict>
            <key>Processor</key>
            <string>EndOfCheckPhase</string>
        </dict>
    </array>
    <!-- TODO: Add checking of some sort-->
</dict>
</plist>